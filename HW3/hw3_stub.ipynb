    "def betaUUpdate(ratingsPerUser, alpha, betaU, betaI, lamb):\n",
    "    newBetaU = {}\n",
    "    for u, items in ratingsPerUser.items():\n",
    "        denom = lamb + len(items)\n",
    "        if denom == 0:\n",
    "            newBetaU[u] = 0.0\n",
    "            continue\n",
    "        total = 0.0\n",
    "        for b,r in items:\n",
    "            total += r - (alpha + betaI.get(b, 0.0))\n",
    "        newBetaU[u] = total / denom\n",
    "    return newBetaU\n"
   ]
  },
