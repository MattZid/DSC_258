   "source": [
    "def betaIUpdate(ratingsPerItem, alpha, betaU, betaI, lamb):\n",
    "    newBetaI = {}\n",
    "    for b, users in ratingsPerItem.items():\n",
    "        denom = lamb + len(users)\n",
    "        if denom == 0:\n",
    "            newBetaI[b] = 0.0\n",
    "            continue\n",
    "        total = 0.0\n",
    "        for u,r in users:\n",
    "            total += r - (alpha + betaU.get(u, 0.0))\n",
    "        newBetaI[b] = total / denom\n",
    "    return newBetaI\n"
   ]
