    "# import ast\n",
    "\n",
    "# ## read in .json.gz file\n",
    "# ## assignment_1/train_Category.json.gz read this file into a variable called data\n",
    "# df = '../assignment_1/train_Category.json.gz'\n",
    "# with gzip.open(df, 'rt', encoding='utf-8') as f:\n",
    "#     data = [ast.literal_eval(line) for line in f]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "e74ac9ec",
   "metadata": {},
   "outputs": [],
   "source": [
    "def readGz(path):\n",
    "    for l in gzip.open(path, 'rt'):\n",
    "        yield eval(l)\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "673a6ca8",
   "metadata": {},
   "outputs": [],
   "source": [
    "def readCSV(path):\n",
    "    f = gzip.open(path, 'rt')\n",
    "    f.readline()\n",
    "    for l in f:\n",
    "        u,b,r = l.strip().split(',')\n",
    "        r = int(r)\n",
    "        yield u,b,r"
   ]
  },
