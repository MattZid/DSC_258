    "def msePlusReg(ratingsTrain, alpha, betaU, betaI, lamb):\n",
    "    se = 0.0\n",
    "    for u,b,r in ratingsTrain:\n",
    "        bu = betaU.get(u, 0.0)\n",
    "        bi = betaI.get(b, 0.0)\n",
    "        err = r - (alpha + bu + bi)\n",
    "        se += err * err\n",
    "    mse = se / len(ratingsTrain)\n",
    "\n",
    "    regularizer = 0.0\n",
    "    for val in betaU.values():\n",
    "        regularizer += val * val\n",
    "    for val in betaI.values():\n",
    "        regularizer += val * val\n",
    "\n",
    "    return mse, mse + lamb * regularizer"
   ]
